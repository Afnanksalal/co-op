# Co-Op RAG Service Configuration

# Database (Neon PostgreSQL)
DATABASE_URL="postgres://user:password@host/database?sslmode=require"

# Supabase (Storage Access)
SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_SERVICE_KEY="eyJ..."
SUPABASE_STORAGE_BUCKET="documents"

# Upstash Vector (768 dimensions, Cosine distance)
UPSTASH_VECTOR_REST_URL="https://your-index.upstash.io"
UPSTASH_VECTOR_REST_TOKEN="your-token"

# Google AI (Embeddings - text-embedding-004)
GOOGLE_AI_API_KEY="your-google-ai-key"

# Security
RAG_API_KEY="your-secure-api-key"

# CORS (Optional)
CORS_ORIGINS="https://your-backend.onrender.com"

# Optional Compression Provider (uses HuggingFace Inference API - no local download)
# Supports: huggingface, clara (both use HF Inference API)
COMPRESSION_ENABLED="false"
COMPRESSION_PROVIDER="huggingface"
COMPRESSION_MODEL="mistralai/Mistral-7B-Instruct-v0.2"
HUGGINGFACE_API_KEY="hf_your_api_key_here"
